.b-header
	position relative
	width 100%
	height 150px
	margin auto
	clearfix()
	&__item
		float left
	&__logo
		position relative
		margin-top 40px
		margin-left 50px
		width 166px
		height 71px
		background url('../img/logo.png') center no-repeat
	&__logo:hover
		cursor pointer
		opacity .7
	&__nav
		position relative
		margin 50px 40px
		list-style none
		&__item
			display inline-block
